<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@title) do |f| %>
        <div class="row form-group">
    　　<%= f.label :name %>
    　　<%= f.text_field :name %>
        </div>
        <div class="row form-group">
    　　<%= f.label :image %>
    　　<%= f.file_field :image %>
    　　</div>
    　　<%= f.submit "登録" %>
    <% end %>
</div>    